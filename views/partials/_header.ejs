<style>
  .btn_login_header {
  padding: 6px 18px;
  color:#222222;
  background-color:transparent;
  border:2px #222222 solid;
  border-radius:5px;
}
#navbarNavAltMarkup {
justify-content: space-between;
}
</style>
<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <tr>
          <td>
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
       </td>
      </div>
      <div class="div_rightCorner_header" >
        <td >
         <% if(user) { %>
          <form action="/logout" method="POST">
            Logged In As: <%= user.email %> !&#160&#160<button type="submit" class="btn_login_header" >Logout</button>
          </form>
        <% } else { %>
          <div class="navbar-nav">
         <a class="nav-item nav-link" href="/login">Login Account</a>
         <a class="nav-item nav-link" href="/register">Register Account</a>
        </div>
        <% } %>
        </td>
        </tr>
      </div>
    </div>
  </nav>
</header>
